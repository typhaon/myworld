<!DOCTYPE html>

<table style="font-size: 1px; line-height: 0;" cellspacing="0" cellpadding ="0" margin ="0" border="0">

  <% @cells.each do |row| %>
    <tr>
    <% row.each do |cell| %>

      <td style="height:4px !important; width:4px;" bgcolor=<%=cell.create_terrain(cell.terrain_id)%>> &nbsp;&nbsp;</td>
    <% end %>
  </tr>
  <% end %>
</table>





